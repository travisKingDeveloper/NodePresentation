<!DOCTYPE html>
<html>
  <head>
    <title>Users</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>Users</h1>
    <p>Welcome to the User Page</p>
    <table style="width: 100%">
        <tr>
            <th>
                Name
            </th>
            <th>
                User Name
            </th>
            <th>
                Password
            </th>
            <th>
                Admin
            </th>
        </tr>

        <% users.forEach(function (user) { %>
            <tr>
                <td>
                    <%= user.name %>
                </td>
                <td>
                    <%= user.username %>
                </td>
                <td>
                    <%= user.password %>
                </td>
                <td>
                    <%= user.admin ? "ADMIN" : "NOT ADMIN" %>
                </td>
            </tr>
        <% }) %>


    </table>

  </body>
</html>
